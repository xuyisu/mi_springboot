    <sql id="Base_Column_List">
        `id`,
        `address_id`,
        `create_time`,
        `update_time`,
        `create_user`,
        `update_user`,
        `delete_flag`,
        `default_flag`,
        `receive_name`,
        `receive_phone`,
        `province`,
        `province_code`,
        `city`,
        `city_code`,
        `area`,
        `area_code`,
        `street`,
        `postal_code`,
        `address_label`
